{
  "name": "Climb-Developer",
  "version": "2.2.0",
  "description": "A new Webpack boilerplate with hot reloading React components, and error handling on module and component level.",
  "scripts": {
    "clean": "rimraf build",
    "build:webpack": "webpack --config webpack.config.prod.js",
    "build": "react-scripts build",
    "start": "react-scripts start",
    "debug": "cross-env NODE_PATH='source' echo 'Nothing? `npm install -g iron-node`' && iron-node source/debug.js",
    "test": "react-scripts test",
    "watch": "watch 'clear && npm run clean && npm run test -s' source",
    "update": "updtr"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wingerwingman/ClimbDeveloper.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "boilerplate",
    "webpack",
    "babel",
    "react-transform"
  ],
  "author": "Justin Winger <justin@wingerdesigns.com> (http://github.com/wingerwingman)",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/wingerwingman/ClimbDeveloper.git/issues"
  },
  "homepage": "https://github.com/wingerwingman/ClimbDeveloper.git",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "6.2.4",
    "babel-plugin-react-html-attrs": "^3.0.5",
    "babel-plugin-react-transform": "2.0.2",
    "babel-plugin-transform-runtime": "6.7.5",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-stage-0": "6.5.0",
    "cheerio": "0.20.0",
    "cross-env": "^3.1.3",
    "css-loader": "^5.0.2",
    "deep-freeze": "0.0.1",
    "react-addons-test-utils": "15.0.1",
    "redbox-react": "1.2.3",
    "rimraf": "2.5.2",
    "updtr": "0.1.10",
    "watch": "0.17.1",
    "webpack": "^5.23.0",
    "webpack-dev-middleware": "1.6.1"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "express": "^4.17.1",
    "history": "^5.0.0",
    "npm-watch": "^0.7.0",
    "react": "^17.0.0",
    "react-bootstrap": "^1.5.0",
    "react-country-region-selector": "^3.0.2",
    "react-css-modules": "^4.0.3",
    "react-dom": "^17.0.0",
    "react-history": "^0.18.2",
    "react-redux": "^4.4.6",
    "react-router": "^3.2.0",
    "react-router-bootstrap": "^0.25.0",
    "react-router-dom": "^5.2.0",
    "react-router-redux": "^4.0.7",
    "react-scripts": "4.0.1",
    "redux": "3.5.2",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.1.0",
    "style-loader": "^0.13.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
